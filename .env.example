# Environment Variables for PulseB2B

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-service-role-key-here

# Proxy Configuration
PROXY_MODE=free  # Options: free, smartproxy, brightdata, none

# SmartProxy (opcional)
SMARTPROXY_USERNAME=
SMARTPROXY_PASSWORD=

# BrightData (opcional)
BRIGHTDATA_USERNAME=
BRIGHTDATA_PASSWORD=

# Webhook Notifications
SLACK_WEBHOOK_URL=
DISCORD_WEBHOOK_URL=
NOTIFICATION_EMAIL=your-email@example.com

# Telegram (opcional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# AWS Configuration (para local testing)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Application Settings
NODE_ENV=development
LOG_LEVEL=info
